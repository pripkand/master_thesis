Cell[TextData[{
 "Takes an extra parameter, the Hamiltonian, and returns a matrix of the form \
",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["M", "TI"], ":", "\[LongEqual]", 
         SubsuperscriptBox[
          RowBox[{"(", 
            RowBox[{"\[LeftAngleBracket]", 
              RowBox[{
                SubsuperscriptBox[
                 StyleBox["O", "TI"], 
                 StyleBox["i", "TI"], "\[Dagger]"], 
                RowBox[{"[", 
                  RowBox[{
                    StyleBox["H", "TI"], ",", 
                    SubscriptBox[
                    StyleBox["O", "TI"], 
                    StyleBox["j", "TI"]]}], "]"}]}], "\[RightAngleBracket]"}],
             ")"}], 
          RowBox[{"(", 
            RowBox[{
              StyleBox["i", "TI"], ",", 
              StyleBox["j", "TI"]}], ")"}], 
          TemplateBox[{
            RowBox[{
              SubscriptBox[
               StyleBox["B", "TI"], 
               StyleBox["L", "TI"]], 
              StyleBox["x", "TI"], 
              SubscriptBox[
               StyleBox["B", "TI"], 
               StyleBox["L", "TI"]]}]}, "Abs"]]}], TraditionalForm], 
     "errors" -> {}, "input" -> 
     "\\mathcal{M}:= (\\langle\\mathcal {O^{\\dagger}} _i[H,\\mathcal {O} _j \
]\\rangle)^{| \\mathcal {B} _L x \\mathcal {B} _L|} _ {(i, j) }", "state" -> 
     "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "2b1eb3cd-6678-4f7c-a44a-15541c89026e"]
}], "Text",
 CellChangeTimes->{{3.9682523320170097`*^9, 3.968252367420545*^9}},
