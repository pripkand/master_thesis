              RowBox[{
               RowBox[{
                RowBox[{"-", 
                 RowBox[{"Sqrt", "[", 
                  RowBox[{"abscissas", "[", 
                   RowBox[{"[", "j", "]"}], "]"}], "]"}]}], "*", 
                RowBox[{"tYMatrices", "[", 
                 RowBox[{"[", "j", "]"}], "]"}]}], ",", 
               RowBox[{
                RowBox[{"-", 
                 RowBox[{"abscissas", "[", 
                  RowBox[{"[", "j", "]"}], "]"}]}], "*", 
                RowBox[{"tYMatrices", "[", 
                 RowBox[{"[", "j", "]"}], "]"}]}]}], "}"}]}], "}"}]}]}], "\[VectorGreaterEqual]",
          "0"}], "\[IndentingNewLine]", ",", 
        RowBox[{"{", 
         RowBox[{"j", ",", "1", ",", "m"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "Compress", " ", "contraints", " ", "into", " ", "a", " ", "singular", " ",
        "one"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"constraints", "=", 
      RowBox[{
       RowBox[{"And", "@@", 
        RowBox[{"Join", "[", 
         RowBox[{"zConditions", ",", "tConditions"}], "]"}]}], "&&", 
       "equalityConditions"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{
