Cell[BoxData[
 RowBox[{
  RowBox[{"generateODaggerCommutatorHOMatrix", "[", 
   RowBox[{"operatorBasis_", ",", "H_"}], "]"}], ":=", 
  RowBox[{"Table", "[", "\[IndentingNewLine]", 
   RowBox[{
