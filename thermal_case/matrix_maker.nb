Cell[BoxData[
 RowBox[{
  RowBox[{"produceSwingerDyson", "[", 
   RowBox[{"operatorWordList_", ",", "H_"}], "]"}], ":=", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"internalList", "=", 
       RowBox[{"{", "}"}]}], ",", "expectationValueList"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Do", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"internalList", "=", 
        RowBox[{"DeleteDuplicates", "[", 
         RowBox[{"DeleteCases", "[", 
          RowBox[{
           RowBox[{"Join", "[", 
            RowBox[{"internalList", ",", 
             RowBox[{"{", 
              RowBox[{"NCExpandReplaceRepeated", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"operator", "**", "H"}], "-", 
                 RowBox[{"H", "**", "operator"}]}], ",", "commuationRules"}], 
               "]"}], "}"}]}], "]"}], ",", "0"}], "]"}], "]"}]}], 
       "\[IndentingNewLine]", ",", 
       RowBox[{"{", 
        RowBox[{"operator", ",", "operatorWordList"}], "}"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"expectationValueList", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"Thread", "[", 
         RowBox[{
          RowBox[{"expectationValue", "[", "item", "]"}], "==", "0"}], "]"}], 
        ",", 
        RowBox[{"{", 
         RowBox[{"item", ",", "internalList"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", "expectationValueList"}]}], "\[IndentingNewLine]",
    "]"}]}]], "Input",
