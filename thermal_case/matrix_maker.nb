Cell["logApproximation ( not used)", "Subsubsection",
 CellChangeTimes->{{3.968422200822035*^9, 3.968422206661133*^9}, {
  3.980332053869692*^9, 3.9803320602024193`*^9}},
 Background->RGBColor[
  1, 0.5, 0.5],ExpressionUUID->"51b4cb3b-c124-4a7b-9268-b3f9c9aea72b"],

Cell[TextData[{
 "Implements the approximation of the logarithm including the \"scrunching \
down towards 1\" factors ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubsuperscriptBox[
          StyleBox["r", "TI"], 
          StyleBox["m", "TI"], 
          StyleBox["k", "TI"]], "\[LongEqual]", 
         SuperscriptBox["2", 
          StyleBox["k", "TI"]], 
         SubscriptBox[
          StyleBox["r", "TI"], 
          StyleBox["m", "TI"]], 
         RowBox[{"(", 
           SuperscriptBox[
            StyleBox["x", "TI"], 
            RowBox[{"1", "/", 
              SuperscriptBox["2", 
               StyleBox["k", "TI"]]}]], ")"}]}], TraditionalForm], 
     "errors" -> {}, "input" -> "$r_m^k=2^k r_m(x^{1/2^k})$", "state" -> 
     "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "c34ce3df-76d2-4c52-90fe-1266f1512047"]
}], "Text",
 CellChangeTimes->{{3.968593693155314*^9, 3.968593757740238*^9}, {
  3.968593806429915*^9, 3.9685938243812847`*^9}},
 Background->RGBColor[
  1, 0.5, 0.5],ExpressionUUID->"d3f56d76-d8e0-4a11-9fb1-cc1ea13576da"],

Cell[BoxData[
 RowBox[{
  RowBox[{"logApproximation", "[", 
   RowBox[{"m_", ",", "k_"}], "]"}], ":=", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"rm", "=", 
      RowBox[{"rmFunction", "[", "m", "]"}]}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"Function", "[", 
     RowBox[{"x", ",", 
      RowBox[{
       RowBox[{"2", "^", "k"}], "*", 
       RowBox[{"rm", "[", 
        RowBox[{"x", "^", 
         RowBox[{"(", 
          RowBox[{"1", "/", 
           RowBox[{"(", 
            RowBox[{"2", "^", "k"}], ")"}]}], ")"}]}], "]"}]}]}], "]"}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{{3.968422208657597*^9, 3.968422281493833*^9}, {
  3.968586475230926*^9, 3.968586477353303*^9}},
