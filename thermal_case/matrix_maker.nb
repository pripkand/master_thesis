Cell[BoxData[
 RowBox[{
  RowBox[{"kmsConstraints", "[", 
   RowBox[{"m_", ",", "k_", ",", "A_", ",", "B_", ",", "C_"}], "]"}], ":=", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"(*", 
      RowBox[{
      "Get", " ", "the", " ", "size", " ", "of", " ", "the", " ", "input", " ",
        "matrices"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"n", "=", 
       RowBox[{"Length", "[", "A", "]"}]}], ",", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{"Define", " ", "the", " ", "ZX"}], ",", 
        RowBox[{"ZY", " ", "and", " ", "TX"}], ",", 
        RowBox[{
         RowBox[{
         "TY", " ", "matrices", " ", "symbolically", " ", "such", " ", "that",
           " ", "Z"}], "=", 
         RowBox[{"ZX", "+", 
          RowBox[{"i", "*", "ZY", " ", "same", " ", "for", " ", 
           RowBox[{"T", "."}]}]}]}]}], " ", "*)"}], "\[IndentingNewLine]", 
      "zXMatrices", ",", "\[IndentingNewLine]", "tXMatrices", ",", 
      "\[IndentingNewLine]", "zYMatrices", ",", "\[IndentingNewLine]", 
      "tYMatrices", ",", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
       "Get", " ", "the", " ", "abscissas", " ", "and", " ", "weights"}], 
       "*)"}], "\[IndentingNewLine]", 
      RowBox[{"abscissas", "=", 
       RowBox[{"Join", "[", 
        RowBox[{
         RowBox[{"{", "0", "}"}], ",", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{"(", 
             RowBox[{"#", "+", "1"}], ")"}], "/", "2"}], "&"}], "/@", 
          RowBox[{"abscissas", "[", "m", "]"}]}]}], "]"}]}], ",", 
      RowBox[{"weights", "=", 
       RowBox[{"Join", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"1", "/", 
           RowBox[{"m", "^", "2"}]}], "}"}], ",", 
         RowBox[{"getWeights", "[", "m", "]"}]}], "]"}]}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{"Define", " ", "other", " ", "variables"}], "*)"}], 
      "\[IndentingNewLine]", "equalityConditions", ",", "zConditions", ",", 
      "tConditions", ",", "constraints", ",", "variables"}], 
     "\[IndentingNewLine]", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"zXMatrices", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"i", "==", "1"}], ",", "B", ",", 
          RowBox[{"makeSymbolicMatrix", "[", 
           RowBox[{
            RowBox[{"ToExpression", "[", 
             RowBox[{"\"\<ZX\>\"", "<>", 
              RowBox[{"ToString", "[", 
               RowBox[{"i", "-", "1"}], "]"}]}], "]"}], ",", "n", 
            ",", "\"\<symmetric\>\""}], "]"}]}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", 
          RowBox[{"k", "+", "1"}]}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"tXMatrices", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"makeSymbolicMatrix", "[", 
         RowBox[{
          RowBox[{"ToExpression", "[", 
           RowBox[{"\"\<TX\>\"", "<>", 
            RowBox[{"ToString", "[", "j", "]"}]}], "]"}], ",", "n", 
          ",", "\"\<symmetric\>\""}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"j", ",", "1", ",", "m"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"zYMatrices", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"i", "==", "1"}], ",", "0", ",", 
          RowBox[{"makeSymbolicMatrix", "[", 
           RowBox[{
            RowBox[{"ToExpression", "[", 
             RowBox[{"\"\<ZY\>\"", "<>", 
              RowBox[{"ToString", "[", 
               RowBox[{"i", "-", "1"}], "]"}]}], "]"}], ",", "n", 
            ",", "\"\<antisymmetric\>\""}], "]"}]}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", 
          RowBox[{"k", "+", "1"}]}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"tYMatrices", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"makeSymbolicMatrix", "[", 
         RowBox[{
          RowBox[{"ToExpression", "[", 
           RowBox[{"\"\<TY\>\"", "<>", 
            RowBox[{"ToString", "[", "j", "]"}]}], "]"}], ",", "n", 
          ",", "\"\<antisymmetric\>\""}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"j", ",", "1", ",", "m"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "Get", " ", "the", " ", "equality", " ", "condition", " ", "for", " ", 
       "the", " ", "T", " ", "matrices"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"equalityConditions", "=", 
      RowBox[{
       RowBox[{"Sum", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"weights", "[", 
           RowBox[{"[", "i", "]"}], "]"}], "*", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"tXMatrices", "[", 
             RowBox[{"[", "i", "]"}], "]"}], "+", 
            RowBox[{"I", "*", 
             RowBox[{"tYMatrices", "[", 
              RowBox[{"[", "i", "]"}], "]"}]}]}], ")"}]}], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "1", ",", "m"}], "}"}]}], "]"}], "==", 
       RowBox[{
        RowBox[{"-", 
         RowBox[{"2", "^", 
          RowBox[{"(", 
           RowBox[{"-", "k"}], ")"}]}]}], "*", "C"}]}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "Get", " ", "the", " ", "block", " ", "matrix", " ", "conditions", " ", 
       "for", " ", "the", " ", "Z", " ", "and", " ", "T", " ", "matrices"}], 
      "*)"}], "\[IndentingNewLine]", 
     RowBox[{"zConditions", "=", 
      RowBox[{"Table", "[", " ", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
              RowBox[{"zXMatrices", "[", 
               RowBox[{"[", "i", "]"}], "]"}], ",", 
              RowBox[{"zXMatrices", "[", 
               RowBox[{"[", 
                RowBox[{"i", "+", "1"}], "]"}], "]"}]}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"zXMatrices", "[", 
               RowBox[{"[", 
                RowBox[{"i", "+", "1"}], "]"}], "]"}], ",", "A"}], "}"}]}], 
           "}"}], "+", 
          RowBox[{"I", "*", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{
               RowBox[{"zYMatrices", "[", 
                RowBox[{"[", "i", "]"}], "]"}], ",", 
               RowBox[{"zYMatrices", "[", 
                RowBox[{"[", 
                 RowBox[{"i", "+", "1"}], "]"}], "]"}]}], "}"}], ",", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{"zYMatrices", "[", 
                RowBox[{"[", 
                 RowBox[{"i", "+", "1"}], "]"}], "]"}], ",", "0"}], "}"}]}], 
            "}"}]}]}], "\[VectorGreaterEqual]", "0"}], "\[IndentingNewLine]", 
        ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "1", ",", "k"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"tConditions", "=", 
      RowBox[{"Table", "[", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"{", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
              RowBox[{
               RowBox[{"zXMatrices", "[", 
                RowBox[{"[", 
                 RowBox[{"k", "+", "1"}], "]"}], "]"}], "-", "A", "-", 
               RowBox[{"tXMatrices", "[", 
                RowBox[{"[", "j", "]"}], "]"}]}], ",", 
              RowBox[{
               RowBox[{"-", 
                RowBox[{"Sqrt", "[", 
                 RowBox[{"abscissas", "[", 
                  RowBox[{"[", "j", "]"}], "]"}], "]"}]}], "*", 
               RowBox[{"tXMatrices", "[", 
                RowBox[{"[", "j", "]"}], "]"}]}]}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{
               RowBox[{"-", 
                RowBox[{"Sqrt", "[", 
                 RowBox[{"abscissas", "[", 
                  RowBox[{"[", "j", "]"}], "]"}], "]"}]}], "*", 
               RowBox[{"tXMatrices", "[", 
                RowBox[{"[", "j", "]"}], "]"}]}], ",", 
              RowBox[{"A", "-", 
