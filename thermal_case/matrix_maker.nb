Cell["\<\
Solve relations takes a list of relations, solves them and applies \
simplification rules derived from those solutions to all of them, eliminating \
dependent variables and cataloguing the independent. Returns the solutions \
and the independent variables\
\>", "Text",
 CellChangeTimes->{{3.980161073297339*^9, 3.980161148180808*^9}},
 Background->RGBColor[
  1, 0.9, 1],ExpressionUUID->"c7a9f0fd-9a58-4b2b-93eb-583375e647d1"],

Cell[BoxData[
 RowBox[{
  RowBox[{"solveRelations", "[", "relationList_", "]"}], ":=", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"solved", "=", 
       RowBox[{"{", "}"}]}], ",", 
      RowBox[{"independentVariables", "=", 
       RowBox[{"{", "}"}]}], ",", 
      RowBox[{"notSolved", "=", "relationList"}], ",", 
      RowBox[{"solutions", "=", 
       RowBox[{"{", "}"}]}], ",", 
      RowBox[{"backup", "=", "0"}], ",", "symbols", ",", 
      RowBox[{"troubleshoot", "=", "False"}], ",", "solution", ",", 
      "independent", ",", "dependent"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Length", "[", "notSolved", "]"}], "!=", "0"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"backup", "==", 
           RowBox[{"4", "*", 
            RowBox[{"Length", "[", "relationList", "]"}]}]}], ",", 
          "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"Print", "[", "\"\<Oops\>\"", "]"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"Break", "[", "]"}]}]}], "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{"troubleshoot", ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"Print", "[", "\"\<==========\>\"", "]"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"Print", "[", "\"\<Not Solved\>\"", "]"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"Print", "[", "notSolved", "]"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"Print", "[", "\"\<Length\>\"", "]"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"Print", "[", 
            RowBox[{"Length", "[", "notSolved", "]"}], "]"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"Print", "[", "\"\<Solved\>\"", "]"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"Print", "[", "solved", "]"}], ";", "\[IndentingNewLine]", 
           
           RowBox[{"Print", "[", "\"\<Solutions\>\"", "]"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"Print", "[", "solutions", "]"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"Print", "[", "\"\<Independent Variables\>\"", "]"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"Print", "[", "independentVariables", "]"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"Print", "[", "\"\<==========\>\"", "]"}]}]}], 
         "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
        RowBox[{"Do", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{
           RowBox[{"If", "[", 
            RowBox[{"troubleshoot", ",", 
             RowBox[{
              RowBox[{"Print", "[", "\"\<---InDo---\>\"", "]"}], ";"}]}], 
            "]"}], ";", "\[IndentingNewLine]", 
           RowBox[{"symbols", "=", 
            RowBox[{"getSymbols", "[", "relations", "]"}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{"troubleshoot", ",", 
             RowBox[{
              RowBox[{"Print", "[", "\"\<Got Symbols\>\"", "]"}], ";", 
              "\[IndentingNewLine]", 
              RowBox[{"Print", "[", "symbols", "]"}]}]}], "]"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"Length", "[", "symbols", "]"}], "==", "1"}], ",", 
             "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"solution", "=", 
               RowBox[{"Flatten", "[", 
                RowBox[{"Solve", "[", "relations", "]"}], "]"}]}], ";", 
              "\[IndentingNewLine]", 
              RowBox[{"solved", "=", 
               RowBox[{"Join", "[", 
                RowBox[{"solved", ",", 
                 RowBox[{"{", "relations", "}"}]}], "]"}]}], ";", 
              "\[IndentingNewLine]", 
              RowBox[{"solutions", "=", 
               RowBox[{"DeleteDuplicates", "[", 
                RowBox[{"Join", "[", 
                 RowBox[{"solutions", ",", "solution"}], "]"}], "]"}]}], ";", 
              "\[IndentingNewLine]", 
              RowBox[{"If", "[", 
               RowBox[{"troubleshoot", ",", "\[IndentingNewLine]", 
                RowBox[{
                 RowBox[{"Print", "[", "\"\<++++++++++\>\"", "]"}], ";", 
                 "\[IndentingNewLine]", 
                 RowBox[{"Print", "[", "\"\<Current Length is 1\>\"", "]"}], ";",
                  "\[IndentingNewLine]", 
                 RowBox[{"Print", "[", "\"\<Symbols\>\"", "]"}], ";", 
                 "\[IndentingNewLine]", 
                 RowBox[{"Print", "[", "symbols", "]"}], ";", 
                 "\[IndentingNewLine]", 
                 RowBox[{"Print", "[", "\"\<Relation\>\"", "]"}], ";", 
                 "\[IndentingNewLine]", 
                 RowBox[{"Print", "[", "relations", "]"}], ";", 
                 "\[IndentingNewLine]", 
                 RowBox[{"Print", "[", "\"\<Solution\>\"", "]"}], ";", 
                 "\[IndentingNewLine]", 
                 RowBox[{"Print", "[", "solution", "]"}], ";", 
                 "\[IndentingNewLine]", 
                 RowBox[{"Print", "[", "\"\<++++++++++\>\"", "]"}]}]}], 
               "\[IndentingNewLine]", "]"}], ";"}]}], "\[IndentingNewLine]", 
            "]"}], ";", "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"Length", "[", "symbols", "]"}], "==", "2"}], ",", 
             "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"independent", "=", 
               RowBox[{"{", 
                RowBox[{"First", "[", 
                 RowBox[{"MinimalBy", "[", 
                  RowBox[{"symbols", ",", "countWordLength"}], "]"}], "]"}], 
                "}"}]}], ";", "\[IndentingNewLine]", 
              RowBox[{"independentVariables", "=", 
               RowBox[{"DeleteDuplicates", "[", 
                RowBox[{"Join", "[", 
                 RowBox[{"independentVariables", ",", "independent"}], "]"}], 
                "]"}]}], ";", "\[IndentingNewLine]", 
              RowBox[{"dependent", "=", 
               RowBox[{"Complement", "[", 
                RowBox[{"symbols", ",", "independentVariables"}], "]"}]}], ";",
               "\[IndentingNewLine]", 
              RowBox[{"solved", "=", 
               RowBox[{"Join", "[", 
                RowBox[{"solved", ",", 
                 RowBox[{"{", "relations", "}"}]}], "]"}]}], ";", 
              "\[IndentingNewLine]", 
              RowBox[{"solutions", "=", 
               RowBox[{"DeleteDuplicates", "[", 
                RowBox[{"Join", "[", 
                 RowBox[{"solutions", ",", 
                  RowBox[{"Flatten", "[", 
                   RowBox[{"Solve", "[", 
                    RowBox[{"relations", ",", "dependent"}], "]"}], "]"}]}], 
                 "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
              RowBox[{"If", "[", 
               RowBox[{"troubleshoot", ",", "\[IndentingNewLine]", 
                RowBox[{
                 RowBox[{"Print", "[", "\"\<++++++++++\>\"", "]"}], ";", 
                 "\[IndentingNewLine]", 
                 RowBox[{"Print", "[", "\"\<Current Length is 2\>\"", "]"}], ";",
                  "\[IndentingNewLine]", 
                 RowBox[{"Print", "[", "\"\<Symbols\>\"", "]"}], ";", 
                 "\[IndentingNewLine]", 
                 RowBox[{"Print", "[", "symbols", "]"}], ";", 
                 "\[IndentingNewLine]", 
                 RowBox[{"Print", "[", "\"\<Relation\>\"", "]"}], ";", 
                 "\[IndentingNewLine]", 
                 RowBox[{"Print", "[", "relations", "]"}], ";", 
                 "\[IndentingNewLine]", 
                 RowBox[{"Print", "[", "\"\<Solution\>\"", "]"}], ";", 
                 "\[IndentingNewLine]", 
                 RowBox[{"Print", "[", "solution", "]"}], ";", 
                 "\[IndentingNewLine]", 
                 RowBox[{"Print", "[", "\"\<Independent Variable\>\"", "]"}], 
