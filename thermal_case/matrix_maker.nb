Cell[TextData[{
 "Returns a matrix of the form ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["M", "TI"], ":", "\[LongEqual]", 
         SubsuperscriptBox[
          RowBox[{"(", 
            RowBox[{"\[LeftAngleBracket]", 
              RowBox[{
                SubsuperscriptBox[
                 StyleBox["O", "TI"], 
                 StyleBox["i", "TI"], "\[Dagger]"], 
                SubscriptBox[
                 StyleBox["O", "TI"], 
                 StyleBox["j", "TI"]]}], "\[RightAngleBracket]"}], ")"}], 
          RowBox[{"(", 
            RowBox[{
              StyleBox["i", "TI"], ",", 
              StyleBox["j", "TI"]}], ")"}], 
          TemplateBox[{
            RowBox[{
              SubscriptBox[
               StyleBox["B", "TI"], 
               StyleBox["L", "TI"]], 
              StyleBox["x", "TI"], 
              SubscriptBox[
               StyleBox["B", "TI"], 
               StyleBox["L", "TI"]]}]}, "Abs"]]}], TraditionalForm], 
     "errors" -> {}, "input" -> 
     "\\mathcal{M}:= (\\langle\\mathcal {O^{\\dagger}} _i\\mathcal {O} _j \
\\rangle)^{| \\mathcal {B} _L x \\mathcal {B} _L|} _ {(i, j) }", "state" -> 
     "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "0747df61-e236-421b-b867-4b10f26b58de"]
}], "Text",
