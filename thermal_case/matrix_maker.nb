Cell[BoxData[
 RowBox[{
  RowBox[{"rmFunction", "[", "m_", "]"}], ":=", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"abscissas", "=", 
       RowBox[{"abscissas", "[", "m", "]"}]}], ",", 
      RowBox[{"weights", "=", 
       RowBox[{"getWeights", "[", "m", "]"}]}]}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"Function", "[", 
     RowBox[{"x", ",", 
      RowBox[{
       RowBox[{
        RowBox[{"1", "/", 
         RowBox[{"m", "^", "2"}]}], "*", 
        RowBox[{"(", 
         RowBox[{"x", "-", "1"}], ")"}]}], "+", 
       RowBox[{"Sum", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"weights", "[", 
           RowBox[{"[", "i", "]"}], "]"}], "*", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"x", "-", "1"}], ")"}], "/", 
