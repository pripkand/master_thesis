Cell[TextData[{
 "Returns the full list of abscissas pairs after shifting them into the [0,1] \
interval with the transformation: ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["x", "TI"], 
          StyleBox["j", "TI"]], "\[Rule]", 
         FractionBox[
          RowBox[{
            SubscriptBox[
             StyleBox["x", "TI"], 
             StyleBox["j", "TI"]], "+", "1"}], "2"], ",", 
         SubscriptBox[
          StyleBox["w", "TI"], 
          StyleBox["j", "TI"]], "\[Rule]", 
         FractionBox[
          SubscriptBox[
           StyleBox["w", "TI"], 
           StyleBox["j", "TI"]], "2"]}], TraditionalForm], "errors" -> {}, 
     "input" -> 
     "$x_j\\rightarrow\\frac{x_j+1}{2},w_j\\rightarrow\\frac{w_j}{2}$", 
     "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "dcb92669-0516-46b1-8f0a-06d1589cc228"],
 ". The endpoint weight is given by ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["w", "TI"], "1"], "\[LongEqual]", 
         FractionBox["2", 
          SuperscriptBox[
           StyleBox["m", "TI"], "2"]]}], TraditionalForm], "errors" -> {}, 
     "input" -> "$w_1=\\frac{2}{m^2}$", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "3891f09c-f06e-44da-9cf3-1bfa8d4b467c"],
 " and the (before transformation) corresponding abscissa is -1."
}], "Text",
 CellChangeTimes->{{3.973158286011922*^9, 3.973158373162258*^9}, {
  3.973159428368918*^9, 3.973159455690192*^9}},
 Background->RGBColor[
  1, 0.9, 1],ExpressionUUID->"72b7b33e-763e-4461-ae48-aa24ff0215ae"],

Cell[BoxData[
 RowBox[{
  RowBox[{"getQuadrature", "[", "m_", "]"}], ":=", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"abscissas", "=", 
       RowBox[{"getAbscissas", "[", "m", "]"}]}], ",", "pairs"}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"pairs", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"{", 
         RowBox[{"x", ",", 
          RowBox[{"getWeight", "[", 
           RowBox[{"m", ",", "x"}], "]"}]}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"x", ",", "abscissas"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"#", "[", 
             RowBox[{"[", "1", "]"}], "]"}], "+", "1"}], ")"}], "/", "2"}], ",", 
         RowBox[{
          RowBox[{"#", "[", 
           RowBox[{"[", "2", "]"}], "]"}], "/", "2"}]}], "}"}], "&"}], "/@", 
      RowBox[{"SortBy", "[", 
       RowBox[{
        RowBox[{"Join", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"-", "1"}], ",", 
             RowBox[{"2", "/", 
              RowBox[{"m", "^", "2"}]}]}], "}"}], "}"}], ",", "pairs"}], 
         "]"}], ",", "First"}], "]"}]}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.9731583781296673`*^9, 3.973158573894799*^9}, {
   3.973158707364253*^9, 3.97315874143863*^9}, {3.973158906122096*^9, 
