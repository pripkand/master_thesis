Cell[TextData[{
 "Returns a matrix of the form  ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["M", "TI"], ":", "\[LongEqual]", 
         SubsuperscriptBox[
          RowBox[{"(", 
            RowBox[{"\[LeftAngleBracket]", 
              RowBox[{
                SubscriptBox[
                 StyleBox["O", "TI"], 
                 StyleBox["i", "TI"]], 
                SubsuperscriptBox[
                 StyleBox["O", "TI"], 
                 StyleBox["j", "TI"], "\[Dagger]"]}], 
              "\[RightAngleBracket]"}], ")"}], 
          RowBox[{"(", 
            RowBox[{
              StyleBox["i", "TI"], ",", 
              StyleBox["j", "TI"]}], ")"}], 
          TemplateBox[{
            RowBox[{
              SubscriptBox[
               StyleBox["B", "TI"], 
               StyleBox["L", "TI"]], 
              StyleBox["x", "TI"], 
              SubscriptBox[
               StyleBox["B", "TI"], 
               StyleBox["L", "TI"]]}]}, "Abs"]]}], TraditionalForm], 
     "errors" -> {}, "input" -> 
     "\\mathcal{M}:= (\\langle\\mathcal {O} _i\\mathcal {O^{\\dagger}} _j \
\\rangle)^{| \\mathcal {B} _L x \\mathcal {B} _L|} _ {(i, j) }", "state" -> 
     "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "b66e5580-2ee5-43a8-b29f-92b204e9d021"]
}], "Text",
 CellChangeTimes->{{3.96825218144173*^9, 3.9682521919491653`*^9}, {
  3.968252362522997*^9, 3.968252363464429*^9}},
 Background->RGBColor[
  1, 0.9, 1],ExpressionUUID->"73d524f8-0349-4d36-bc9b-a680ee0f0445"],

Cell[BoxData[
 RowBox[{
  RowBox[{"generateOODaggerMatrix", "[", "operatorBasis_", "]"}], ":=", 
  RowBox[{"Table", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"expectationValue", "[", 
     RowBox[{"NCExpandReplaceRepeated", "[", 
      RowBox[{
       RowBox[{"operatorj", "**", 
        RowBox[{"aj", "[", "operatori", "]"}]}], ",", "commuationRules"}], 
      "]"}], "]"}], ",", "\[IndentingNewLine]", 
    RowBox[{"{", 
     RowBox[{"operatori", ",", "operatorBasis"}], "}"}], ",", 
    RowBox[{"{", 
