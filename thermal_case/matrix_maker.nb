     RowBox[{
      RowBox[{"aj", "[", "x", "]"}], "->", "x"}], ",", "\[IndentingNewLine]", 
     
     RowBox[{
      RowBox[{"aj", "[", "p", "]"}], "->", "p"}]}], "\[IndentingNewLine]", 
