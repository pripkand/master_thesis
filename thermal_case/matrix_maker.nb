Cell["\<\
Takes the powers of x and p of a pair like p^m**x^n and passes p to the other \
side so that all operator pairs have p to the right.\
\>", "Text",
 CellChangeTimes->{{3.967459670835178*^9, 3.967459728899666*^9}, {
  3.9674597803598948`*^9, 3.967459818178451*^9}, {3.968241905531887*^9, 
  3.9682419059543333`*^9}},
 Background->RGBColor[
  1, 0.5, 0.5],ExpressionUUID->"c229e1b5-892a-4209-8475-3b6ea00900d3"],

Cell[BoxData[
 RowBox[{
  RowBox[{"commutationPNXM", "[", 
   RowBox[{"m_", ",", "n_"}], "]"}], ":=", 
  RowBox[{"If", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"m", "==", "1"}], ",", 
    RowBox[{"(*", 
     RowBox[{
     "Checks", " ", "if", " ", "there", " ", "is", " ", "only", " ", "one", " ",
       "power", " ", "of", " ", "p"}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"x", "^", 
       RowBox[{"(", 
        RowBox[{"n", "-", "1"}], ")"}]}], "*", 
      RowBox[{"(", 
       RowBox[{"-", "I"}], ")"}], "*", "n"}], "+", 
     RowBox[{
      RowBox[{"x", "^", 
       RowBox[{"(", "n", ")"}]}], "**", "p"}]}], " ", 
    RowBox[{"(*", 
     RowBox[{
     "If", " ", "there", " ", "is", " ", "only", " ", "one", " ", "power", " ",
       "it", " ", "just", " ", "passes", " ", "p", " ", "to", " ", "the", " ",
       "right", " ", "of", " ", "the", " ", "powers", " ", "of", " ", "x"}], 
     "*)"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"commutationPNXM", "[", 
       RowBox[{
        RowBox[{"m", "-", "1"}], ",", 
        RowBox[{"n", "-", "1"}]}], "]"}], " ", "*", 
      RowBox[{"(", 
       RowBox[{"-", "I"}], ")"}], "*", "n"}], "+", 
     RowBox[{
      RowBox[{"commutationPNXM", "[", 
       RowBox[{
        RowBox[{"m", "-", "1"}], ",", "n"}], "]"}], "**", "p"}]}]}], 
   RowBox[{"(*", 
    RowBox[{
    "If", " ", "there", " ", "are", " ", "more", " ", "than", " ", "one", " ",
      "powers", " ", "of", " ", "p", " ", "then", " ", "it", " ", "passes", " ",
      "them", " ", "all", " ", "to", " ", "the", " ", "right", " ", 
     "recursively"}], "*)"}], "\[IndentingNewLine]", "]"}]}]], "Input",
 CellChangeTimes->{{3.961405833967626*^9, 3.961405892786327*^9}, 
   3.961406087276746*^9, {3.961412158048513*^9, 3.961412199823617*^9}, {
   3.967459490853785*^9, 3.967459517606564*^9}, {3.9682419547790537`*^9, 
   3.96824203946828*^9}},
 Background->RGBColor[1, 0.5, 0.5],
 CellLabel->"In[3]:=",ExpressionUUID->"79e597cc-4573-4fad-9eba-1dc54a93c4d9"]
}, Open  ]],

