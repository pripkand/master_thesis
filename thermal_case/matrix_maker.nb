                RowBox[{"abscissas", "[", 
                 RowBox[{"[", "j", "]"}], "]"}], "*", 
                RowBox[{"tXMatrices", "[", 
                 RowBox[{"[", "j", "]"}], "]"}]}]}]}], "}"}]}], "}"}], " ", "+", 
          RowBox[{"I", "*", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{
               RowBox[{
                RowBox[{"zYMatrices", "[", 
                 RowBox[{"[", 
                  RowBox[{"k", "+", "1"}], "]"}], "]"}], "-", 
                RowBox[{"tYMatrices", "[", 
                 RowBox[{"[", "j", "]"}], "]"}]}], ",", 
               RowBox[{
                RowBox[{"-", 
                 RowBox[{"Sqrt", "[", 
                  RowBox[{"abscissas", "[", 
                   RowBox[{"[", "j", "]"}], "]"}], "]"}]}], "*", 
                RowBox[{"tYMatrices", "[", 
                 RowBox[{"[", "j", "]"}], "]"}]}]}], "}"}], ",", 
