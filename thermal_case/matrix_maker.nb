        RowBox[{
         RowBox[{"ToExpression", "[", "name", "]"}], "[", 
         RowBox[{"j", ",", "i"}], "]"}]}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"i", ",", "1", ",", "size"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"j", ",", "1", ",", "size"}], "}"}]}], "]"}], ",", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"i", "<", "j"}], ",", 
        RowBox[{
         RowBox[{"ToExpression", "[", "name", "]"}], "[", 
         RowBox[{"i", ",", "j"}], "]"}], ",", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"i", "==", "j"}], ",", "0", ",", 
          RowBox[{"-", 
           RowBox[{
            RowBox[{"ToExpression", "[", "name", "]"}], "[", 
            RowBox[{"j", ",", "i"}], "]"}]}]}], "]"}]}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"i", ",", "1", ",", "size"}], "}"}], ",", 
      RowBox[{"{", 
       RowBox[{"j", ",", "1", ",", "size"}], "}"}]}], "]"}]}], 
   "]"}]}]], "Input",
