Cell[TextData[{
 "Returns the quadrature weight with the formula: ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["w", "TI"], 
          StyleBox["j", "TI"]], "\[LongEqual]", 
         FractionBox[
          RowBox[{
            SubscriptBox[
             StyleBox["x", "TI"], 
             StyleBox["j", "TI"]], "-", "1"}], 
          RowBox[{
            SuperscriptBox[
             StyleBox["m", "TI"], "2"], 
            SubsuperscriptBox[
             StyleBox["P", "TI"], 
             RowBox[{
               StyleBox["m", "TI"], "-", "1"}], "2"], 
            RowBox[{"(", 
              SubscriptBox[
               StyleBox["x", "TI"], 
               StyleBox["j", "TI"]], ")"}]}]]}], TraditionalForm], 
     "errors" -> {}, "input" -> "$w_j=\\frac{x_j-1}{m^2 P_{m-1}^2(x_j)}$", 
     "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "f5f0b727-6cb4-4c6e-bd10-c2b779cf6bfb"],
 "where $x_j$ is the corresponding abscissa valid for all non-endpoint \
abscissas. "
}], "Text",
 CellChangeTimes->{{3.973158074270566*^9, 3.9731582655385447`*^9}, {
  3.973159384315123*^9, 3.973159426853505*^9}},
 Background->RGBColor[
  1, 0.9, 1],ExpressionUUID->"35d4e405-004e-41a1-864f-1cc9de92d6a2"],

Cell[BoxData[
 RowBox[{
  RowBox[{"getWeight", "[", 
   RowBox[{"m_", ",", "x_"}], "]"}], ":=", 
  RowBox[{
   RowBox[{"(", 
    RowBox[{"1", "-", "x"}], ")"}], "/", 
   RowBox[{
    RowBox[{"(", 
     RowBox[{"m", "*", 
      RowBox[{"LegendreP", "[", 
       RowBox[{
        RowBox[{"m", "-", "1"}], ",", "x"}], "]"}]}], ")"}], "^", 
    "2"}]}]}]], "Input",
 CellChangeTimes->{{3.968421579001842*^9, 3.968421593055874*^9}, {
   3.9684216466473217`*^9, 3.9684217641317043`*^9}, {3.968583931017874*^9, 
   3.968583941959794*^9}, {3.968584579320636*^9, 3.968584597683806*^9}, {
   3.968584736838641*^9, 3.968584744959482*^9}, {3.968584791728725*^9, 
