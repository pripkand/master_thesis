Cell["\<\
Splits an expression containing operators into a list containing only the \
operator products.\
\>", "Text",
 CellChangeTimes->{{3.967713351676861*^9, 3.967713359485285*^9}, {
  3.967713457189945*^9, 3.9677134732248774`*^9}},
 Background->RGBColor[
  1, 0.9, 1],ExpressionUUID->"56ca6628-1edd-4906-a6dc-55e097951bd7"],

Cell[BoxData[
 RowBox[{
  RowBox[{"NCProductList", "[", "expr_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "terms", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"terms", "=", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Head", "[", "expr", "]"}], "===", "Plus"}], ",", 
        RowBox[{"List", "@@", "expr"}], ",", 
        RowBox[{"{", "expr", "}"}]}], "]"}]}], ";", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{
       "Checks", " ", "if", " ", "the", " ", "Head", " ", "of", " ", "the", " ",
         "expresion", " ", "is", " ", "summation", " ", 
        RowBox[{"e", ".", "i", ".", " ", "expr"}], " ", "of", " ", "the", " ",
         "form", " ", "expr1"}], "+", 
       RowBox[{
        RowBox[{"expr2", ".", " ", "If"}], " ", "it", " ", "is", " ", "then", 
        " ", "it", " ", "splits", " ", "it", " ", "and", " ", "adds", " ", 
        "each", " ", "term", " ", "into", " ", "a", " ", 
        RowBox[{"list", ".", " ", "If"}], " ", "it", " ", "is", " ", "not", " ",
         "then", " ", "wraps", " ", "the", " ", "expression", " ", "in", " ", 
        "a", " ", "list"}]}], "*)"}], "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"terms", "=", 
      RowBox[{
       RowBox[{"terms", "/.", " ", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{
           RowBox[{"c_", "?", "NumericQ"}], " ", "f_"}], ":>", "f"}], ")"}]}],
        "/.", " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"c_", "?", "NumericQ"}], "*", "f_"}], ":>", "f"}], 
        ")"}]}]}], ";", 
     RowBox[{"(*", 
      RowBox[{
      "Gets", " ", "rid", " ", "of", " ", "any", " ", "numerical", " ", 
       "multiplicative", " ", "factors"}], "*)"}], "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"terms", "=", 
      RowBox[{"DeleteCases", "[", 
       RowBox[{"terms", ",", 
        RowBox[{"_", "?", "NumericQ"}]}], "]"}]}], ";", 
     RowBox[{"(*", 
      RowBox[{
      "Deletes", " ", "cases", " ", "where", " ", "a", " ", "number", " ", 
       "is", " ", "left", " ", "on", " ", "its", " ", "own"}], "*)"}], 
     "\[IndentingNewLine]", "\[IndentingNewLine]", "terms"}]}], 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.967711830686174*^9, 3.967711830686304*^9}, 
   3.967711906918715*^9, {3.968218828724605*^9, 3.968218834657217*^9}, 
