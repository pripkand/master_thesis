Cell[BoxData[
 RowBox[{
  RowBox[{"asignDomains", "[", "list_", "]"}], ":=", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"returnList", ",", "\[IndentingNewLine]", 
      RowBox[{"real", "=", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"Xor", "[", 
            RowBox[{
             RowBox[{"StringContainsQ", "[", 
              RowBox[{
               RowBox[{"ToString", "[", "item", "]"}], ",", "\"\<X\>\""}], 
              "]"}], ",", 
             RowBox[{"StringContainsQ", "[", 
              RowBox[{
               RowBox[{"ToString", "[", "item", "]"}], ",", "\"\<P\>\""}], 
              "]"}]}], "]"}], ",", "item"}], "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"item", ",", "list"}], "}"}]}], "]"}]}], ",", 
      "\[IndentingNewLine]", "realRules"}], "\[IndentingNewLine]", "}"}], ",",
     "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"returnList", "=", 
      RowBox[{"Association", "[", 
       RowBox[{"Table", "[", 
        RowBox[{
