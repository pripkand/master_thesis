Cell[TextData[{
 "Takes as argument an integer and returns a list with the proper abscissas \
for the quadrature rule in ascending order. The abscissas are the roots of \
the equation: ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         FractionBox[
          RowBox[{
            SubscriptBox[
             StyleBox["P", "TI"], 
             RowBox[{
               StyleBox["m", "TI"], "-", "1"}]], 
            RowBox[{"(", 
              StyleBox["x", "TI"], ")"}], "+", 
            SubscriptBox[
             StyleBox["P", "TI"], 
             StyleBox["m", "TI"]], 
            RowBox[{"(", 
              StyleBox["x", "TI"], ")"}]}], 
          RowBox[{"1", "+", 
            StyleBox["x", "TI"]}]], "\[LongEqual]", "0"}], TraditionalForm], 
     "errors" -> {}, "input" -> "$\\frac{P_{m-1}(x)+P_{m}(x)}{1+x}=0$", 
     "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "dab720f7-fc03-4073-8739-2f29b3c89559"],
 " where ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["P", "TI"], 
          StyleBox["m", "TI"]], "(", 
         StyleBox["x", "TI"], ")"}], TraditionalForm], "errors" -> {}, 
     "input" -> "$P_{m}(x)$", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "98d54614-f914-4566-92b5-ab511e2c3704"],
 " are the Legendre Polynomials. Some notes on the values: The function \
returns the abscissas ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       SubscriptBox[
        StyleBox["x", "TI"], 
        StyleBox["i", "TI"]], TraditionalForm], "errors" -> {}, "input" -> 
     "$x_i$", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "f04ca282-388f-47bb-ae7c-403cc4d57077"],
 " ."
}], "Text",
 CellChangeTimes->{{3.968332350418891*^9, 3.968332471597148*^9}, {
   3.968332503662463*^9, 3.968332514966568*^9}, {3.9683327664637003`*^9, 
   3.968332849757743*^9}, 3.9731582728960047`*^9},
 Background->RGBColor[
  1, 0.9, 1],ExpressionUUID->"74f967a3-9b56-43cc-8322-778453638230"],

Cell[BoxData[
 RowBox[{
  RowBox[{"getAbscissas", "[", "m_", "]"}], ":=", 
  RowBox[{"Table", "[", 
   RowBox[{
    RowBox[{"items", "[", 
     RowBox[{"[", "2", "]"}], "]"}], ",", 
    RowBox[{"{", 
     RowBox[{"items", ",", 
      RowBox[{"Flatten", "[", 
       RowBox[{"NSolve", "[", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"LegendreP", "[", 
              RowBox[{
               RowBox[{"m", "-", "1"}], ",", "x"}], "]"}], "+", 
             RowBox[{"LegendreP", "[", 
              RowBox[{"m", ",", "x"}], "]"}]}], ")"}], "/", 
           RowBox[{"(", 
            RowBox[{"1", "+", "x"}], ")"}]}], "==", "0"}], ",", "x"}], "]"}], 
       "]"}]}], "}"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.96833193067231*^9, 3.968331931949387*^9}, {
  3.9683319631333847`*^9, 3.968332062394565*^9}, {3.968332568354953*^9, 
