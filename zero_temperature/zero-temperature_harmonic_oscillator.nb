Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"reqRel", "[", "t_", "]"}], ":=", 
   RowBox[{
    RowBox[{
     RowBox[{"2", "*", "EE", "*", 
      RowBox[{"(", 
       RowBox[{"t", "-", "1"}], ")"}], "*", 
      RowBox[{"ToExpression", "[", 
       RowBox[{"\"\<x\>\"", "<>", 
        RowBox[{"ToString", "[", 
         RowBox[{"t", "-", "2"}], "]"}]}], "]"}]}], "+", 
     RowBox[{
      RowBox[{"1", "/", "4"}], "*", 
      RowBox[{"(", 
       RowBox[{"t", "-", "1"}], ")"}], "*", 
      RowBox[{"(", 
       RowBox[{"t", "-", "2"}], ")"}], "*", 
      RowBox[{"(", 
       RowBox[{"t", "-", "3"}], ")"}], "*", 
      RowBox[{"ToExpression", "[", 
       RowBox[{"\"\<x\>\"", "<>", 
        RowBox[{"ToString", "[", 
         RowBox[{"t", "-", "4"}], "]"}]}], "]"}]}], "-", 
     RowBox[{"t", "*", 
      RowBox[{"ToExpression", "[", 
       RowBox[{"\"\<x\>\"", "<>", 
        RowBox[{"ToString", "[", "t", "]"}]}], "]"}]}]}], "==", "0"}]}], 
  ";"}]], "Input",
