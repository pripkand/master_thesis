  1, 0.9, 1],ExpressionUUID->"fa564fb8-8a29-40a9-a26a-a5da3fa9d4d7"],

Cell[BoxData[
 RowBox[{
  RowBox[{"doSDPAtK", "[", 
   RowBox[{"searchspace_", ",", "step_", ",", "k_", ",", 
    RowBox[{"tolerance_", ":", 
     RowBox[{"10", "^", 
      RowBox[{"(", 
       RowBox[{"-", "8"}], ")"}]}]}]}], "]"}], ":=", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{
        RowBox[{
        "Initiates", " ", "Matt", " ", "as", " ", "the", " ", "Hankel", " ", 
         "Matrix", " ", "at", " ", "depth", " ", "K", "\[IndentingNewLine]", 
         RowBox[{"matt", "[", "e", "]"}]}], "-", "tI"}], ">=", "0"}], 
      "\[IndentingNewLine]", "*)"}], "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"matt", "=", 
       RowBox[{"buildMatrix", "[", "k", "]"}]}], ",", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{"Initiates", " ", "Containers"}], "*)"}], 
      "\[IndentingNewLine]", "allowedDataAtK", ",", 
      RowBox[{"fullDataAtK", "=", 
       RowBox[{"{", "}"}]}], ",", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
       "Initiates", " ", "several", " ", "other", " ", "internal", " ", 
        "variables"}], "*)"}], "\[IndentingNewLine]", "constraintMatrix", ",",
       "x2", ",", "t", ",", "tstar", ",", "x2star", ",", "eigenvalue", ",", 
      "xVec", ","}], "\[IndentingNewLine]", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Do", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{
        RowBox[{"constraintMatrix", ":=", 
         RowBox[{"FullSimplify", "[", 
          RowBox[{"matt", "[", "e", "]"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"xVec", "=", 
         RowBox[{"SemidefiniteOptimization", "[", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"-", "t"}], ",", 
           RowBox[{"{", 
            RowBox[{"VectorGreaterEqual", "[", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{"constraintMatrix", ",", 
                RowBox[{"t", " ", 
                 RowBox[{"IdentityMatrix", "[", 
                  RowBox[{"k", "+", "1"}], "]"}]}]}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{"\"\<SemidefiniteCone\>\"", ",", " ", 
                RowBox[{"k", "+", "1"}]}], "}"}]}], "]"}], "}"}], ",", 
           "\[IndentingNewLine]", 
           RowBox[{"{", "t", "}"}], ",", 
           RowBox[{"{", "\"\<PrimalMinimizerVector\>\"", "}"}], ",", 
           RowBox[{"Method", "->", "\"\<MOSEK\>\""}], ",", 
           RowBox[{"Tolerance", "->", "tolerance"}]}], "\[IndentingNewLine]", 
          "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"{", "tstar", "}"}], "=", 
         RowBox[{"{", 
          RowBox[{
           RowBox[{"xVec", "[", 
            RowBox[{"[", "1", "]"}], "]"}], "[", 
           RowBox[{"[", "1", "]"}], "]"}], "}"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"eigenvalue", "=", 
         RowBox[{"Min", "[", 
          RowBox[{"Eigenvalues", "[", 
           RowBox[{"FullSimplify", "[", 
            RowBox[{"N", "[", 
             RowBox[{"matt", "[", "e", "]"}], "]"}], "]"}], "]"}], "]"}]}], ";",
         "\[IndentingNewLine]", 
        RowBox[{"fullDataAtK", "=", 
         RowBox[{"Join", "[", 
          RowBox[{"fullDataAtK", ",", 
           RowBox[{"{", 
            RowBox[{"{", 
             RowBox[{"e", ",", "tstar", ",", "eigenvalue"}], "}"}], "}"}]}], 
          "]"}]}], ";"}], "\[IndentingNewLine]", ",", 
       RowBox[{"{", 
        RowBox[{"e", ",", 
         RowBox[{"searchspace", "[", 
          RowBox[{"[", "1", "]"}], "]"}], ",", 
         RowBox[{"searchspace", "[", 
          RowBox[{"[", 
           RowBox[{"-", "1"}], "]"}], "]"}], ",", "step"}], "}"}]}], "]"}], ";",
      "\[IndentingNewLine]", 
     RowBox[{"allowedDataAtK", "=", 
      RowBox[{"allowedEnergies", "[", "fullDataAtK", "]"}]}], ";", 
     "\[IndentingNewLine]", "allowedDataAtK"}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
