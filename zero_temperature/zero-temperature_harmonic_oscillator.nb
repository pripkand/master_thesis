Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"buildMatrix", "[", "k_Integer", "]"}], ":=", 
   RowBox[{"Function", "[", 
    RowBox[{
     RowBox[{"{", "e", "}"}], ",", 
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"rels", ",", "rules", ",", "xi", ",", "sol", ",", "mat"}], 
        "}"}], ",", "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{"Generate", " ", "relation", " ", "equations"}], "*)"}], 
       RowBox[{
        RowBox[{"rels", "=", 
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"reqRel", "[", 
            RowBox[{"2", " ", "j"}], "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"j", ",", "2", ",", 
             RowBox[{"k", "+", "1"}]}], "}"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"(*", 
         RowBox[{"Build", " ", "rules", " ", "step", " ", "by", " ", "step"}],
          "*)"}], 
        RowBox[{"rules", "=", 
         RowBox[{"{", "}"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"Do", "[", 
         RowBox[{
          RowBox[{"If", "[", 
           RowBox[{
            RowBox[{"i", ">", "0"}], ",", 
            RowBox[{
             RowBox[{"xi", "=", 
              RowBox[{"ToExpression", "[", 
               RowBox[{"\"\<x\>\"", "<>", 
                RowBox[{"ToString", "[", 
                 RowBox[{
                  RowBox[{"2", " ", "i"}], "+", "2"}], "]"}]}], "]"}]}], ";", 
             "\[IndentingNewLine]", 
             RowBox[{"sol", "=", 
              RowBox[{"First", "[", 
               RowBox[{"Solve", "[", 
                RowBox[{
                 RowBox[{
                  RowBox[{"rels", "[", 
                   RowBox[{"[", "i", "]"}], "]"}], "/.", " ", "rules"}], ",", 
                 "xi"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
             RowBox[{"rules", "=", 
              RowBox[{"FullSimplify", "[", 
               RowBox[{"Join", "[", 
                RowBox[{"rules", ",", "sol"}], "]"}], "]"}]}]}], ",", 
            "\[IndentingNewLine]", 
            RowBox[{"(*", 
             RowBox[{"else", " ", "base", " ", 
              RowBox[{"case", ":", 
               RowBox[{"i", "==", "0"}]}]}], "*)"}], "\[IndentingNewLine]", 
            RowBox[{"rules", "=", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{
                RowBox[{"ToExpression", "[", "\"\<x0\>\"", "]"}], "->", "1"}],
                ",", 
               RowBox[{
                RowBox[{"ToExpression", "[", "\"\<x2\>\"", "]"}], "->", 
                "EE"}]}], "}"}]}]}], "]"}], ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "0", ",", "k"}], "}"}]}], "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"(*", 
         RowBox[{"Build", " ", "the", " ", "matrix"}], "*)"}], 
        "\[IndentingNewLine]", 
        RowBox[{"mat", "=", 
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"OddQ", "[", 
              RowBox[{"i", "+", "j"}], "]"}], ",", "0", ",", 
             RowBox[{"If", "[", 
              RowBox[{
               RowBox[{
                RowBox[{"i", "==", "0"}], "&&", 
                RowBox[{"j", "==", "0"}]}], ",", "1", ",", 
               RowBox[{"ToExpression", "[", 
                RowBox[{"\"\<x\>\"", "<>", 
                 RowBox[{"ToString", "[", 
                  RowBox[{"i", "+", "j"}], "]"}]}], "]"}]}], "]"}]}], "]"}], ",", 
           RowBox[{"{", 
            RowBox[{"i", ",", "0", ",", "k"}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"j", ",", "0", ",", "k"}], "}"}]}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"(*", 
         RowBox[{"Substitute", " ", "rules", " ", "and", " ", "parameters"}], 
         "*)"}], "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"mat", "/.", " ", "rules"}], "/.", " ", 
         RowBox[{"{", 
          RowBox[{"EE", "->", "e"}], "}"}]}]}]}], "]"}]}], "]"}]}], 
  ";"}]], "Input",
